# Board configuration
CONFIG_BOARD_VILI=y

# Fingerprint
CONFIG_FINGERPRINT_FPC_TEE=m
CONFIG_FINGERPRINT_GOODIX_TA=m

# Haptics
CONFIG_INPUT_AW8697_HAPTIC=y

# Power supply
CONFIG_BQ_FUEL_GAUGE=y
CONFIG_DUAL_FUEL_GAUGE=y

# Proximity
CONFIG_MIUS_PROXIMITY=y

# Touchscreen
CONFIG_TOUCHSCREEN_ST_FTS_V521_SPI=y
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=y

# Time of flight
CONFIG_STMVL53L5=m

# WiFi
CONFIG_CNSS_QCA6490=y
